    "sass-watch": "concurrently \"npm run sass-watch-layout\" \"npm run sass-watch-theme\"",
    "sass-watch-layout": "node-sass -w src/assets/layout/css -o src/assets/layout/css",
    "sass-watch-theme": "node-sass -w src/assets/theme -o src/assets/theme"
